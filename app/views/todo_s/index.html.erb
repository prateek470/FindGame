<p id="notice"><%= notice %></p>

<h1>Todo S</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Notes</th>
      <th>Place</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @todo_s.each do |todo_| %>
      <tr>
        <td><%= todo_.title %></td>
        <td><%= todo_.notes %></td>
        <td><%= todo_.place %></td>
        <td><%= link_to 'Show', todo_ %></td>
        <td><%= link_to 'Edit', edit_todo__path(todo_) %></td>
        <td><%= link_to 'Destroy', todo_, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Todo ', new_todo__path %>
